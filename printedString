var flavor = "strawberry";
var vessel = "cone";
var toppings = "sprinkles";

//gMeta
var gimMe = ("I'd like two scoops of " + flavor + " ice cream in a " + vessel + " with " + toppings + ".");

if ((flavor == "vanilla" || flavor == "chocolate") && (vessel == "cone" || vessel == "bowl") && (toppings == "sprinkles" || toppings == "peanuts")) {
    console.log(gimMe);
} else 
	console.log("I don't like these options.");
